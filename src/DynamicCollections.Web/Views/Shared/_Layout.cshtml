	<script type="text/javascript">window.jQuery || document.write('<script src="../../Scripts/jquery-2.0.3.min.js">\x3C/script>')</script>
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
	<script type="text/javascript">window.jQuery.ui || document.write('<script src="../../Scripts/jquery-ui-1.10.3.custom.min.js">\x3C/script>')</script>

	@RenderSection("scripts", required: false)
